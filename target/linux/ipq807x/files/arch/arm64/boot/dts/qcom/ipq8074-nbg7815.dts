		local-mac-address = [00 00 00 00 00 00];
		phy-handle = <&aqr113c_5>;
		phy-mode = "sgmii";
	};
};

&blsp1_i2c2 {
	pinctrl-0 = <&i2c_0_pins>;
	pinctrl-names = "default";
	status = "okay";

  lp5569@32 {
          #address-cells = <1>;
          #size-cells = <0>;
          compatible = "ti,lp5569";
          label = "q1";
          reg = <0x32>;
  
          clock-mode = /bits/ 8 <1>; /* LP55XX_CLOCK_AUTO */

          multi-led@0 {
                  color = <LED_COLOR_ID_RGB>;
                  #address-cells = <1>;
                  #size-cells = <0>;
                  reg = <0x0>;
                  led@0 {
                          led-cur = /bits/ 8 <0xDF>;
                          max-cur = /bits/ 8 <0xFF>;
                          reg = <0x0>;
                          color = <LED_COLOR_ID_GREEN>;
                  };

                  led@1 {
                          led-cur = /bits/ 8 <0xDF>;
                          max-cur = /bits/ 8 <0xFF>;
                          reg = <0x1>;
                          color = <LED_COLOR_ID_BLUE>;
                  };

                  led@6 {
                          led-cur = /bits/ 8 <0xDF>;
                          max-cur = /bits/ 8 <0xFF>;
                          reg = <0x6>;
                          color = <LED_COLOR_ID_RED>;
                  };
          };
          multi-led@1 {
                    color = <LED_COLOR_ID_RGB>;
                    #address-cells = <1>;
                    #size-cells = <0>;
                    reg = <0x1>;
                    led@2 {
                            led-cur = /bits/ 8 <0xDF>;
                            max-cur = /bits/ 8 <0xFF>;
                            reg = <0x2>;
                            color = <LED_COLOR_ID_GREEN>;
                    };

                    led@3 {
                            led-cur = /bits/ 8 <0xDF>;
                            max-cur = /bits/ 8 <0xFF>;
                            reg = <0x3>;
                            color = <LED_COLOR_ID_BLUE>;
                    };

                    led@7 {
                            led-cur = /bits/ 8 <0xDF>;
                            max-cur = /bits/ 8 <0xFF>;
                            reg = <0x7>;
                            color = <LED_COLOR_ID_RED>;
                    };
            };
            multi-led@2 {
                    color = <LED_COLOR_ID_RGB>;
                    #address-cells = <1>;
                    #size-cells = <0>;
                    reg = <0x2>;
                    led@4 {
                            led-cur = /bits/ 8 <0xDF>;
                            max-cur = /bits/ 8 <0xFF>;
                            reg = <0x4>;
                            color = <LED_COLOR_ID_GREEN>;
                    };

                    led@5 {
                            led-cur = /bits/ 8 <0xDF>;
                            max-cur = /bits/ 8 <0xFF>;
                            reg = <0x5>;
                            color = <LED_COLOR_ID_BLUE>;
                    };

                    led@8 {
                            led-cur = /bits/ 8 <0xDF>;
                            max-cur = /bits/ 8 <0xFF>;
                            reg = <0x8>;
                            color = <LED_COLOR_ID_RED>;
                    };
            };
        };

        lp5569@35 {
                compatible = "ti,lp5569";
                #address-cells = <1>;
                #size-cells = <0>;
                label = "q2";
                reg = <0x35>;
                clock-mode = /bits/ 8 <1>; /* LP55XX_CLOCK_AUTO */


                multi-led@0 {
                        color = <LED_COLOR_ID_RGB>;
                        #address-cells = <1>;
                        #size-cells = <0>;
                        reg = <0x0>;
                        led@0 {
                                led-cur = /bits/ 8 <0xDF>;
                                max-cur = /bits/ 8 <0xFF>;
                                reg = <0x0>;
                                color = <LED_COLOR_ID_GREEN>;
                        };

                        led@1 {
                                led-cur = /bits/ 8 <0xDF>;
                                max-cur = /bits/ 8 <0xFF>;
                                reg = <0x1>;
                                color = <LED_COLOR_ID_BLUE>;
                        };

                        led@6 {
                                led-cur = /bits/ 8 <0xDF>;
                                max-cur = /bits/ 8 <0xFF>;
                                reg = <0x6>;
                                color = <LED_COLOR_ID_RED>;
                        };
                };
                multi-led@1 {
                        color = <LED_COLOR_ID_RGB>;
                        #address-cells = <1>;
                        #size-cells = <0>;
                        reg = <0x1>;
                        led@2 {
                                led-cur = /bits/ 8 <0xDF>;
                                max-cur = /bits/ 8 <0xFF>;
                                reg = <0x2>;
                                color = <LED_COLOR_ID_GREEN>;
                        };

                        led@3 {
                                led-cur = /bits/ 8 <0xDF>;
                                max-cur = /bits/ 8 <0xFF>;
                                reg = <0x3>;
                                color = <LED_COLOR_ID_BLUE>;
                        };

                        led@7 {
                                led-cur = /bits/ 8 <0xDF>;
                                max-cur = /bits/ 8 <0xFF>;
                                reg = <0x7>;
                                color = <LED_COLOR_ID_RED>;
                        };
                };
                multi-led@2 {
                        color = <LED_COLOR_ID_RGB>;
                        #address-cells = <1>;
                        #size-cells = <0>;
                        reg = <0x2>;
                        led@4 {
                                led-cur = /bits/ 8 <0xDF>;
                                max-cur = /bits/ 8 <0xFF>;
                                reg = <0x4>;
                                color = <LED_COLOR_ID_GREEN>;
                        };

                        led@5 {
                                led-cur = /bits/ 8 <0xDF>;
                                max-cur = /bits/ 8 <0xFF>;
                                reg = <0x5>;
                                color = <LED_COLOR_ID_BLUE>;
                        };

                        led@8 {
                                led-cur = /bits/ 8 <0xDF>;
                                max-cur = /bits/ 8 <0xFF>;
                                reg = <0x8>;
                                color = <LED_COLOR_ID_RED>;
                        };
                };
        };

	tmp103@70 {
		compatible = "ti,tmp103";
		reg = <0x70>;
	};
};

