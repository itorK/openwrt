&soc {
	dp1: dp1 {
		device_type = "network";
		compatible = "qcom,nss-dp";
		qcom,id = <1>;
		reg = <0x3a001000 0x200>;
		qcom,mactype = <0>;
		local-mac-address = [00 00 00 00 00 00];
		phy-handle = <&qca8075_1>;
		phy-mode = "sgmii";
	};

	dp2: dp2 {
		device_type = "network";
		compatible = "qcom,nss-dp";
		qcom,id = <2>;
		reg = <0x3a001200 0x200>;
		qcom,mactype = <0>;
		local-mac-address = [00 00 00 00 00 00];
		phy-handle = <&qca8075_2>;
		phy-mode = "sgmii";
		};

	dp3: dp3 {
		device_type = "network";
		compatible = "qcom,nss-dp";
		qcom,id = <3>;
		reg = <0x3a001400 0x200>;
		qcom,mactype = <0>;
		local-mac-address = [00 00 00 00 00 00];
		phy-handle = <&qca8075_3>;
		phy-mode = "sgmii";
	};

	dp4: dp4 {
		device_type = "network";
		compatible = "qcom,nss-dp";
		qcom,id = <4>;
		reg = <0x3a001600 0x200>;
		qcom,mactype = <0>;
		local-mac-address = [00 00 00 00 00 00];
		phy-handle = <&qca8075_4>;
		phy-mode = "sgmii";
